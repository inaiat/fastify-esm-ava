  "devDependencies": {
    "@swc/core": "^1.2.210",
    "@swc/helpers": "^0.4.3",
    "@types/node": "^18.0.0",
    "ava": "^4.3.0",
    "c8": "^7.11.3",
    "nodemon": "^2.0.18",
    "pino-pretty": "^8.1.0",
    "regenerator-runtime": "^0.13.9",
